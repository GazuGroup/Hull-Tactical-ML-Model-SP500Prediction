   "source": [
    "class ParticipantVisibleError(Exception):\n",
    "    pass\n",
    "\n",
    "\n",
    "def score(solution: pd.DataFrame, submission: pd.DataFrame, row_id_column_name: str) -> float:\n",
    "    \"\"\"\n",
    "    Calculates a custom evaluation metric (volatility-adjusted Sharpe ratio)\n",
    "    adjusted for your dataset's column names.\n",
    "\n",
    "    This metric penalizes strategies that take on significantly more volatility\n",
    "    than the underlying market.\n",
    "\n",
    "    Returns:\n",
    "        float: The calculated adjusted Sharpe ratio.\n",
    "    \"\"\"\n",
    "\n",
    "    # --- 1️⃣ Basic validation ---\n",
    "    if not pd.api.types.is_numeric_dtype(submission['prediction']):\n",
    "        raise ParticipantVisibleError('Predictions must be numeric')\n",
    "\n",
    "    # Align prediction (position) with the solution DataFrame\n",
    "    solution = solution.copy()\n",
    "    solution['position'] = submission['prediction']\n",
    "\n",
    "    # --- ✅ Replace clipping with rescaling ---\n",
    "    # Rescale predictions to fill [0, 2] range (min → 0, max → 2)\n",
    "    min_pred, max_pred = solution['position'].min(), solution['position'].max()\n",
    "    if max_pred == min_pred:\n",
    "        raise ParticipantVisibleError(\"Predictions are constant — cannot rescale to range [0, 2].\")\n",
    "\n",
    "    solution['position'] = (\n",
    "        2 * (solution['position'] - min_pred) / (max_pred - min_pred)\n",
    "    ).clip(0, 2)\n",
    "\n",
    "    # --- 2️⃣ Strategy returns ---\n",
    "    solution['strategy_returns'] = (\n",
    "        solution['lagged_risk_free_rate'] * (1 - solution['position']) +\n",
    "        solution['position'] * solution['lagged_forward_returns']\n",
    "    )\n",
    "\n",
    "    # --- 3️⃣ Strategy Sharpe ratio components ---\n",
    "    strategy_excess_returns = solution['strategy_returns'] - solution['lagged_risk_free_rate']\n",
    "    strategy_excess_cumulative = (1 + strategy_excess_returns).prod()\n",
    "    strategy_mean_excess_return = strategy_excess_cumulative ** (1 / len(solution)) - 1\n",
    "    strategy_std = solution['strategy_returns'].std()\n",
    "\n",
    "    trading_days_per_yr = 252\n",
    "    if strategy_std == 0:\n",
    "        raise ParticipantVisibleError('Division by zero, strategy std is zero')\n",
    "\n",
    "    sharpe = strategy_mean_excess_return / strategy_std * np.sqrt(trading_days_per_yr)\n",
    "    strategy_volatility = float(strategy_std * np.sqrt(trading_days_per_yr) * 100)\n",
    "\n",
    "    # --- 4️⃣ Market Sharpe ratio components ---\n",
    "    market_excess_returns = solution['lagged_forward_returns'] - solution['lagged_risk_free_rate']\n",
    "    market_excess_cumulative = (1 + market_excess_returns).prod()\n",
    "    market_mean_excess_return = market_excess_cumulative ** (1 / len(solution)) - 1\n",
    "    market_std = solution['lagged_forward_returns'].std()\n",
    "    market_volatility = float(market_std * np.sqrt(trading_days_per_yr) * 100)\n",
    "\n",
    "    if market_volatility == 0:\n",
    "        raise ParticipantVisibleError('Division by zero, market std is zero')\n",
    "\n",
    "    # --- 5️⃣ Volatility penalty ---\n",
    "    excess_vol = max(0, strategy_volatility / market_volatility - 1.2)\n",
    "    vol_penalty = 1 + excess_vol\n",
    "\n",
    "    # --- 6️⃣ Return penalty ---\n",
    "    return_gap = max(\n",
    "        0,\n",
    "        (market_mean_excess_return - strategy_mean_excess_return) * 100 * trading_days_per_yr,\n",
    "    )\n",
    "    return_penalty = 1 + (return_gap**2) / 100\n",
    "\n",
    "    # --- 7️⃣ Adjusted Sharpe ratio ---\n",
    "    adjusted_sharpe = sharpe / (vol_penalty * return_penalty)\n",
    "    adjusted_sharpe = min(float(adjusted_sharpe), 1_000_000)\n",
    "\n",
    "    # --- 📈 Print key metrics ---\n",
    "    print(f\"Sharpe Ratio: {sharpe:.2f}\")\n",
    "    print(f\"Annualized Volatility: {strategy_volatility:.2f}%\")\n",
    "\n",
    "    return adjusted_sharpe\n"
   ]
  },
